    "classix": "^2.2.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-lazy-with-preload": "^2.2.1",
    "react-router-dom": "^7.1.1",
    "swr": "^2.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.17.0",
